{
	"compilerOptions": {
		"module": "none",
		"noImplicitAny": true,
		"removeComments": true,
		"preserveConstEnums": true,
		"outFile": "build/spine-all.js",
		"sourceMap": true,
		"declaration": true,
		"target": "ES2022",
	},
	"include": [
		"src/core/**/*",
		"src/webgl/**/*",
		"src/player/**/*",
		"src/*",
	],
	"exclude": [
		"build"
	]
}